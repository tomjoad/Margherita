/* Column Container stylesheet */

.column-container {
    overflow-x: hidden;
    overflow-y: hidden;
    padding-right: 230px;
    .content {
        float: left;
        margin-left: 0;
        margin-right: -230px;
        overflow-x: hidden;
        overflow-y: hidden;
        width: 100%;
    }
    .aside {
        float: left;
        left: 230px;
        position: relative;
        vertical-align: top;
        width: 225px;
    }
}
