footer {
  margin-top: 0;
  padding-top: 6px;
  padding-bottom: 5px;
  color: #444;
  text-shadow: 0 0px 1px rgba(0, 0, 0, 0.5);
  a {
   -moz-transition: all 0.3s linear 0s;
   -webkit-transition: all 0.3s linear 0s;
   transition: all 0.2s linear 0s;
   font-weight: bold;
   color: #999;
   &:hover {
      color: #333;
      text-decoration: none;
      text-shadow: 0 0 5px rgba(255, 255, 255, 0.2);
     }
  }
  small {
    float: center;
    text-align: center;
  }
}
