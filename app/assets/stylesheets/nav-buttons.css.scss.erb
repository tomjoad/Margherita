/* Categories: navigation buttons stylesheet */

.nav-buttons {
    margin-top: 20px;
    margin-bottom: 20px;
    .btn-group {
        border-radius: 4px;
        .btn {
            color: #333;
        }
        .btn-large {
            width: 8.622%;
            font-size: 14px;
            font-weight: normal;
            text-transform: uppercase;
            text-shadow: 0 1px 1px rgba(255, 255, 255, 0.75);
        }
    }
}


